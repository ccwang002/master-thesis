  number = {6},
  journaltitle = {Nature Reviews Genetics},
  shortjournal = {Nat Rev Genet},
  author = {Nielsen, Rasmus and Paul, Joshua S. and Albrechtsen, Anders and Song, Yun S.},
  urldate = {2016-07-04},
