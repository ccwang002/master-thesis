  volume = {11},
  url = {http://dx.doi.org/10.1186/gb-2010-11-8-r86},
  shorttitle = {Galaxy},
  abstract = {Increased reliance on computational approaches in the life sciences has revealed grave concerns about how accessible and reproducible computation-reliant results truly are. Galaxy                   http://usegalaxy.org                                  , an open web-based platform for genomic research, addresses these problems. Galaxy automatically tracks and manages data provenance and provides support for capturing the context and intent of computational methods. Galaxy Pages are interactive, web-based documents that provide users with a medium to communicate a complete computational analysis.},
  timestamp = {2016-07-04T13:51:54Z},
  journaltitle = {Genome Biology},
  shortjournal = {Genome Biology},
  author = {Goecks, Jeremy and Nekrutenko, Anton and Taylor, James},
  urldate = {2016-07-04},
  date = {2010},
  pages = {R86}
}

@article{griffith2015:genome,
  title = {Genome {{Modeling System}}: {{A Knowledge Management Platform}} for {{Genomics}}},
  volume = {11},
  url = {http://dx.doi.org/10.1371%2Fjournal.pcbi.1004274},
  abstract = {In this work, we present the Genome Modeling System (GMS), an analysis information management system capable of executing automated genome analysis pipelines at a massive scale. The GMS framework provides detailed tracking of samples and data coupled with reliable and repeatable analysis pipelines. The GMS also serves as a platform for bioinformatics development, allowing a large team to collaborate on data analysis, or an individual researcher to leverage the work of others effectively within its data management system. Rather than separating ad-hoc analysis from rigorous, reproducible pipelines, the GMS promotes systematic integration between the two. As a demonstration of the GMS, we performed an integrated analysis of whole genome, exome and transcriptome sequencing data from a breast cancer cell line (HCC1395) and matched lymphoblastoid line (HCC1395BL). These data are available for users to test the software, complete tutorials and develop novel GMS pipeline configurations. The GMS is available at https://github.com/genome/gms.},
  timestamp = {2016-07-04T13:56:02Z},
  number = {7},
  journaltitle = {PLoS Comput Biol},
  shortjournal = {PLoS Comput Biol},
  author = {Griffith, Malachi and Griffith, Obi L. and Smith, Scott M. and Ramu, Avinash and Callaway, Matthew B. and Brummett, Anthony M. and Kiwala, Michael J. and Coffman, Adam C. and Regier, Allison A. and Oberkfell, Ben J. and Sanderson, Gabriel E. and Mooney, Thomas P. and Nutter, Nathaniel G. and Belter, Edward A. and Du, Feiyu and Long, Robert L. and Abbott, Travis E. and Ferguson, Ian T. and Morton, David L. and Burnett, Mark M. and Weible, James V. and Peck, Joshua B. and Dukes, Adam and McMichael, Joshua F. and Lolofie, Justin T. and Derickson, Brian R. and Hundal, Jasreet and Skidmore, Zachary L. and Ainscough, Benjamin J. and Dees, Nathan D. and Schierding, William S. and Kandoth, Cyriac and Kim, Kyung H. and Lu, Charles and Harris, Christopher C. and Maher, Nicole and Maher, Christopher A. and Magrini, Vincent J. and Abbott, Benjamin S. and Chen, Ken and Clark, Eric and Das, Indraniel and Fan, Xian and Hawkins, Amy E. and Hepler, Todd G. and Wylie, Todd N. and Leonard, Shawn M. and Schroeder, William E. and Shi, Xiaoqi and Carmichael, Lynn K. and Weil, Matthew R. and Wohlstadter, Richard W. and Stiehr, Gary and McLellan, Michael D. and Pohl, Craig S. and Miller, Christopher A. and Koboldt, Daniel C. and Walker, Jason R. and Eldred, James M. and Larson, David E. and Dooling, David J. and Ding, Li and Mardis, Elaine R. and Wilson, Richard K.},
